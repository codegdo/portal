{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "codegdo/portal:nginx-${ENV}",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "express",
        "react"
      ],
      "memory": 128
    },
    {
      "name": "express",
      "image": "codegdo/portal:express-${ENV}",
      "hostname": "express",
      "essential": false,
      "memory": 128
    },
    {
      "name": "react",
      "image": "codegdo/portal:react-${ENV}",
      "react": "react",
      "essential": false,
      "memory": 128
    }
  ]
}