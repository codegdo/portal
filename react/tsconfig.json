{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "allowJs": true,
    "jsx": "preserve",
    "outDir": "./dist",
    "noEmit": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "types": ["react", "react-dom", "react-router-dom"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*"]
}
